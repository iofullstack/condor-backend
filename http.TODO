  ## Role
  POST:
    /api/roles
      @send {
        title: 'It's title'
      }
      @return {
        message: 'Role saved',
        savedRole: Role
      }

  ## USER
  POST:
    /api/users
      @send
        {
          ci
          exp
          firstName
          lastName
          avatar: 'base64'
          email
          password
          gender
          birthdate
          address
          cellphone
          createdAt: Date
          role: 'Waiter'
          permits: ['Permiso1','Permiso3','Permiso3','Permiso4']
        }
      @return
        {
          message: 'User saved',
          savedUser: User
        }
  
  ## CLIENT
  POST:
    /api/clients
      @send
        {
          nit_passport,
          firstName,
          lastName
        }
      @return
        {
          message: 'Client saved'
          savedClient: Client
        }

